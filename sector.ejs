  <!-- JavaScript for Search and Reset -->
  <script>
    document.getElementById('datasetSearchBtn').addEventListener('click', function() {
      const query = document.getElementById('datasetSearch').value.trim();
      if (query) {
        alert(`Searching for datasets: ${query}`);
        // Add your search functionality here
      } else {
        alert("Please enter a search term for datasets.");
      }
    });
  
    document.getElementById('sectorSearchBtn').addEventListener('click', function() {
      const query = document.getElementById('sectorSearch').value.trim();
      if (query) {
        alert(`Searching for sectors: ${query}`);
        // Add your search functionality here
      } else {
        alert("Please enter a search term for sectors.");
      }
    });
  
    document.getElementById('resetBtn').addEventListener('click', function() {
      document.getElementById('sectorSearch').value = '';
      alert('Search field reset.');
    });
  </script>
   
